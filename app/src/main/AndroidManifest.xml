<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.ithacacounseling.moviesapp1" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.ithacacounseling.popularmovies11.CustomArrayAdapter"
            android:label="@string/title_activity_custom_array_adapter" >
        </activity>
        <activity
            android:name="org.ithacacounseling.popularmovies11.Grid_Item_Movies"
            android:label="@string/title_activity_grid__item__movies" >
        </activity>
    </application>

</manifest>
